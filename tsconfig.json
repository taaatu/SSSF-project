{
    "compilerOptions": {
      "outDir": "dist",
      "sourceMap": true,
      "target": "esnext",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitAny": true,
      "strict": true,
      "skipLibCheck": true,
      "typeRoots": [
        "node_modules/@types",
      ],
    },
    "include": ["./*.js", "graphql/src/**/*.ts", "graphql/test/**/*.ts"]
  }